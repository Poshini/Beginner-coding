#include <iostream>
#include<bits/stdc++.h>
using namespace std;
int main() 
{
  int n,sum=0;
  cout<<"Enter the number of elements: ";
  cin>>n;
  int a[n];
  cout<<"Enter the elements: ";
  for(int i=0;i<n;i++)
  {
    cin>>a[i];
    sum += a[i];
  }
  cout<<"Sum is: "<<sum;
  return 0;
}
    OUTPUT
Enter the number of elements: 6
Enter the elements: 12 23 89 10 45 67
Sum is: 246
