#include<iostream>
#include<math.h>
using namespace std;
 
int main()
{
    int num;
    double ans;
 
    cout<<"Enter number: ";
    cin>>num;
 
    ans=(double)pow((double)num,(double)1/(double)3);
 
    cout<<"\nCube Root of "<<num<<" is: "<<ans<<"\n";
 
    return 0;
}
      OUTPUT
Enter number: 458
Cube Root of 458 is 7.70824
