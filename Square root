#include<iostream>
#include<math.h>
 
using namespace std;
 
int main()
{
    float sq,n;
 
    cout<<"Enter any positive number : ";
    cin>>n;
 
    sq=sqrt(n);
 
    cout<<"\nSquare  root of "<<n<<" is "<<sq<<"\n";
 
    return 0;
}
      OUTPUT
Enter any positive number : 56
Suare root of 56 is 7.48331
