#include<iostream>
using namespace std;
 
int main()
{
        int n, i, arr[50], sum=0;
        cout<<"\nHow many number you want to enter : ";
        cin>>n;

        for(i=0; i<n; i++)
        {
            cout<<"\nEnter [ "<<i+1<<" ] number : ";
                cin>>arr[i];
                sum=sum+arr[i];
        }
 
        int armean=sum/n;
 
        cout<<"\nArithmetic Mean is = "<<armean<<"\n";
 
        return 0;
}
       OUTPUT
 How many numbers do you want to enter : 7
 Enter [1] number : 45
 Enter [2] number : 12
 Enter [3] number : 89
 Enter [4] number : 96
 Enter [5] number : 73
 Enter [6] number : 55
 Enter [7] number : 68
 Arithmetic Mean is = 62
