#include <bits/stdc++.h> 
#include<iostream>
using namespace std; 

void find(string strsent, string strrec) 
{ 
    char rem[5];
    unordered_map<char, int> m; 

    int k=0;
    for (int i = 0; i < strsent.length(); i++) 
        m[strsent[i]]++; 


    for (int i = 0; i < strrec.length(); i++) 
        m[strrec[i]]--; 

    for (auto j = m.begin(); j != m.end(); j++) 
    {  
        if (j->second == 1) {
        rem[k] = j->first;
             k++;}
    } 
    for(int i=0;i<k;i++)
    cout<<rem[i]<<" ";
}
int main() 
{ 
    string strsent;
    cout<<"Enter first string: ";
    getline(cin,strsent);
    cout<<"Enter second string: ";
    string strrec;
    getline(cin,strrec); 
    find(strsent, strrec); 
    return 0;
} 
